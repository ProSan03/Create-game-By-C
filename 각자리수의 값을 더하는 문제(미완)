#include <stdio.h>

int main(){
	int Input, Thausand, Hundred, ten, one;
	scanf_s("%d", &Input);

	if (Input < 10000)
	{
		while (Input)
		{
			Thausand = Input % 1000;
			Hundred = Input % 100;
			ten = Input % 10;
			one = Input % 1;
		}
		printf("%d", Thausand + Hundred + ten + one);
	}
	
	
	return 0;
}
